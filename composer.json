{
    "name": "thenexxuz/canvas",
    "description": "A Laravel publishing platform",
    "keywords": ["canvas", "laravel", "publishing", "platform"],
    "license": "MIT",
    "homepage": "https://cnvs.io",
    "support": {
        "issues": "https://github.com/austintoddj/canvas/issues",
        "source": "https://github.com/austintoddj/canvas"
    },
    "authors": [
        {
            "name": "Todd Austin",
            "email": "austin.todd.j@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "doctrine/dbal": "^2.10",
        "laravel/framework": "^6.0|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Canvas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Canvas\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Canvas\\CanvasServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
